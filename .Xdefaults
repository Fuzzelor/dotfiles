Xft.dpi: 96
Xft.antialias: true
!Xft.rgba: rgb
!Xft.hintin: true
!Xft.hintstyle: hintslight
!URXVT
URxvt.depth: 32
URxvt*font: xft:SauceCodePro\ NF:style=regular:pixelsize=14
URxvt*boldFont: xft:SauceCodePro\ NF:style=bold:pixelsize=14
URxvt.scrollBar: false
!URxvt*transparent: true
!URxvt*shading: 15

! special
*.foreground:   #a0a0a0
*.background:   #232323
*.cursorColor:  #a0a0a0

! black
*.color0:       #303030
*.color8:       #686868

! red
*.color1:       #d370a3
*.color9:       #ffa7da

! green
*.color2:       #6d9e3f
*.color10:      #a3d572

! yellow
*.color3:       #b58858
*.color11:      #efbd8b

! blue
*.color4:       #6095c5
*.color12:      #98cbfe

! magenta
*.color5:       #ac7bde
*.color13:      #e5b0ff

! cyan
*.color6:       #3ba275
*.color14:      #75daa9

! white
*.color7:       #cfcfcf
*.color15:      #ffffff

! ------------------------------------------------------------------------------
! ROFI Color theme
! User: caya
! ------------------------------------------------------------------------------
! "Color scheme for normal row" Set from: Default
rofi.color-normal:                   #232323,#e5b0ff,#232323,#ac7bde,#eee8d5
! "Color scheme for urgent row" Set from: Default
rofi.color-urgent:                   #232323,#e5b0ff,#232323,#ac7bde,#eee8d5
! "Color scheme for active row" Set from: Default
rofi.color-active:                   #232323,#e5b0ff,#232323,#ac7bde,#eee8d5
! "Color scheme window" Set from: Default
rofi.color-window:                   #232323,#e5b0ff
! "Separator style (none, dash, solid)" Set from: Default
rofi.separator-style:                solid
! Font
rofi.font:			     "Source Code Pro" 11
