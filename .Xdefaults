Xft.dpi: 96
Xft.antialias: true
!Xft.rgba: rgb
!Xft.hintin: true
!Xft.hintstyle: hintslight
!URXVT
URxvt.depth: 32
URxvt*font: xft:'Source Code Pro for Powerline':pixelsize=14
URxvt*boldFont: xft:'Source Code Pro for Powerline':pixelsize=14,xft:Source Code Pro:pixelsize=14
URxvt.scrollBar: false
!URxvt*transparent: true
!URxvt*shading: 15

! special
URxvt*foreground:   #586e75
URxvt*background:   [95]#fdf6e3
URxvt*cursorColor:  #586e75

! black
*.color0:       #002b36
*.color8:       #657b83

! red
*.color1:       #dc322f
*.color9:       #dc322f

! green
*.color2:       #859900
*.color10:      #859900

! yellow
*.color3:       #b58900
*.color11:      #b58900

! blue
*.color4:       #268bd2
*.color12:      #268bd2

! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #2aa198

! white
*.color7:       #93a1a1
*.color15:      #fdf6e3

! ------------------------------------------------------------------------------
! ROFI Color theme
! User: caya
! ------------------------------------------------------------------------------
! "Color scheme for normal row" Set from: Default
rofi.color-normal:                   #445566,#ccddee,#445566,#586e75,#eee8d5
! "Color scheme for urgent row" Set from: Default
rofi.color-urgent:                   #445566,#ccddee,#445566,#586e75,#eee8d5
! "Color scheme for active row" Set from: Default
rofi.color-active:                   #445566,#ccddee,#445566,#586e75,#eee8d5
! "Color scheme window" Set from: Default
rofi.color-window:                   #445566,#657b83
! "Separator style (none, dash, solid)" Set from: Default
rofi.separator-style:                dash
! Font
rofi.font:			     "Source Code Pro" 11
